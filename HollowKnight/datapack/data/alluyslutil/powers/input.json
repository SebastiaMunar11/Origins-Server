	"update_axes": {
		"type": "origins:action_over_time",
		"interval": 1,
		"entity_action": {
			"type": "origins:and",
			"actions": [
				{
					"type": "origins:execute_command",
					"command": "resource set @s alluyslutil:input_axis_x 0"
				},
				{
					"type": "origins:execute_command",
					"command": "resource set @s alluyslutil:input_axis_y 0"
				},
				{
					"type": "origins:execute_command",
					"command": "resource set @s alluyslutil:input_axis_z 0"
				},
				{
					"type": "origins:if_else",
					"condition": {
						"type": "origins:resource",
						"resource": "alluyslutil:input_holding_forward",
						"comparison": ">",
						"compare_to": 0
					},
					"if_action": {
						"type": "origins:change_resource",
						"resource": "alluyslutil:input_axis_z",
						"change": 1
					}
				},
				{
					"type": "origins:if_else",
					"condition": {
						"type": "origins:resource",
						"resource": "alluyslutil:input_holding_backward",
						"comparison": ">",
						"compare_to": 0
					},
					"if_action": {
						"type": "origins:change_resource",
						"resource": "alluyslutil:input_axis_z",
						"change": -1
					}
				},
				{
					"type": "origins:if_else",
					"condition": {
						"type": "origins:resource",
						"resource": "alluyslutil:input_holding_left",
						"comparison": ">",
						"compare_to": 0
					},
					"if_action": {
						"type": "origins:change_resource",
						"resource": "alluyslutil:input_axis_x",
						"change": -1
					}
				},
				{
					"type": "origins:if_else",
					"condition": {
						"type": "origins:resource",
						"resource": "alluyslutil:input_holding_right",
						"comparison": ">",
						"compare_to": 0
					},
					"if_action": {
						"type": "origins:change_resource",
						"resource": "alluyslutil:input_axis_x",
						"change": 1
					}
				},
				{
					"type": "origins:if_else",
					"condition": {
						"type": "origins:resource",
						"resource": "alluyslutil:input_holding_jump",
						"comparison": ">",
						"compare_to": 0
					},
					"if_action": {
						"type": "origins:change_resource",
						"resource": "alluyslutil:input_axis_y",
						"change": 1
